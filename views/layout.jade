  body(ng-app="sampleApp" ng-controller="MainController")
    .container       
        .row
            .headerimages               
              img(src='/images/Lines-1.png' class='line')
              img(src='/images/Lines-2.png' class='line')
               
        .row.menurow
            .col-md-4 
                .divLogo
                    a(href="/")
                        img(src='/images/logo300.png' class='logoimg')
                .mobileMenu
                        a(href="" ng-click="doMenu()")
                           i(class="fa fa-bars" aria-hiiden="true")
                      
                        
            .col-md-8
                .divmenuwrap                   
                    ul(class='ulmenu')
                       li
                           a(href="/about") ABOUT
                       li 
                           a(href="/services") SERVICES
                       li 
                           a(href="/tech") TECHNOLOGY
                       li 
                           a(href="/news") NEWS
                       li 
                           a() WORK WITH US
                       li 
                           a() CONTACT
                       li
